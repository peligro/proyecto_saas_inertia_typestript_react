{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "lib": ["DOM", "DOM.Iterable", "ESNext"],
      "esModuleInterop": true,
      "skipLibCheck": true,
      "outDir": "./dist",
      "rootDir": "./resources/js",
      "moduleResolution": "Node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "jsx": "react-jsx",
      "strict": true,
      "baseUrl": ".",
      "types": ["vite/client", "@inertiajs/react/types"]
    },
    "include": ["resources/js/**/*"]
  }